programa {
  funcao inicio() {
    inteiro numero
    escreva("Digite um numero: ")
    leia(numero)
    se(numero < 0){
      escreva("Valor negativo")
      }senao se(numero > 0){
        escreva("Valor positivo")
        }senao{
          escreva("Valor é 0")
        
        }
    
    
    
  }
}
